{
    "name": "mr-feek/laravel-git-hooks",
    "type": "library",
    "description": "git hooks for laravel",
    "keywords": [
        "Feek",
        "LaravelGitHooks"
    ],
    "homepage": "https://github.com/mr-feek/LaravelGitHooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Fiachra McDermott",
            "email": "feek@feek.rocks",
            "homepage": "https://feek.rocks",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "~5.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3"
    },
    "autoload": {
        "psr-4": {
            "Feek\\LaravelGitHooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feek\\LaravelGitHooks\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
